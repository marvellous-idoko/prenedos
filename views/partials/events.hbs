<script>
    let y = document.getElementsByClassName('slider')
    let yo = document.getElementsByClassName('slider1')
    let countre = 0

    function pode(p) {
 //       y = document.getElementsByClassName('slider') 
   //     y = document.getElementsByClassName('slider') 
   
        for (let index = 0; index < 6; index++) {
            y.item(index).classList.remove('showwq');
            yo.item(index).classList.remove('showwq');

        }
        document.getElementById(p).style.display = 'block'
        y.item(countre).classList.add('showwq')
        yo.item(countre).classList.add('showwq')

    }
    function clos(p) {
        document.getElementById(p).style.display = 'none'
        y.item(countre).classList.remove('showwq')
        yo.item(countre).classList.remove('showwq')

    }
    function nxt() {
                y.item(countre).classList.remove('showwq')
            countre++
            if(countre == y.length){
                countre = 0
            }
            y.item(countre).classList.add('showwq')
        
    }

    function nxt1() {
                yo.item(countre).classList.remove('showwq')
            countre++
            if(countre == yo.length){
                countre = 0
            }
            yo.item(countre).classList.add('showwq')
        
    }
    function prev() {
         y.item(countre).classList.remove('showwq')
            countre--
            if(countre == -1){
                countre = y.length - 1
            }
            y.item(countre).classList.add('showwq')
    }
    
    function prev1() {
         yo.item(countre).classList.remove('showwq')
            countre--
            if(countre == -1){
                countre = yo.length - 1
            }
            yo.item(countre).classList.add('showwq')
    }
    document.getElementById('events').click()
    function fg(sas) {
        // buttons
        document.getElementById('gallery').classList.remove('active')
        document.getElementById('events').classList.remove('active')
        document.getElementById(sas).classList.add('active')

        // tabs
        document.getElementById('events1').classList.remove('show')
        document.getElementById('gallery1').classList.remove('show')
        document.getElementById(sas + 1).classList.add('show')
    }
</script>